{
  "name": "store-mvt",
  "version": "1.0.1",
  "description": "An experiment in makeing a set of mbtiles from the output of geojson-vt",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SKalt/store-mvt.git"
  },
  "scripts": {
    "html-doc": "jsdoc -d docs/ --readme README.md src/index.js",
    "md-doc": "node build/make-docs.js",
    "make-docs": "npm run md-doc && npm run html-doc",
    "prepublish": "npm run make-docs",
    "test": "mocha -u tdd test/index.js",
    "posttest": "rm -r test/output/*",
    "debug-test": "DEBUG=this:* mocha -u tdd test/index.js"
  },
  "keywords": [
    "geojson",
    "geojson-vt",
    "mvt",
    "vector tiles",
    "pbf",
    "GIS"
  ],
  "author": "Steven Kalt",
  "license": "MIT",
  "dependencies": {
    "@mapbox/mbtiles": "^0.9.0",
    "@mapbox/tilebelt": "^1.0.1",
    "@turf/bbox": "^5.1.5",
    "@turf/random": "^5.1.0",
    "debug": "^3.1.0",
    "geojson-vt": "^3.0.0",
    "handlebars": "^4.0.0",
    "supercluster": "^3.0.1",
    "vt-pbf": "^3.1.0"
  },
  "devDependencies": {
    "@mapbox/vector-tile": "^1.3.0",
    "@turf/helpers": "^5.1.5",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.13.0",
    "eslint-config-google": "^0.9.1",
    "jsdoc": "^3.5.5",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-to-markdown": "^3.0.2",
    "mocha": "^4.0.1"
  }
}
